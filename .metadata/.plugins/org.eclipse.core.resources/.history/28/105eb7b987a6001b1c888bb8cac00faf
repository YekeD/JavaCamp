
public class Main {

	public static void main(String[] args) {
		//Kategori
		Category category1 = new Category(01,"Yazýlým");
		Category[] categories = {category1};

		//Kurslar
		Course course1 = new Course(1,"Yazýlým Geliþtirme Kampý","Engin Demirog","C# + Angler");
		Course course2 = new Course(2,"Yazýlým Geliþtirici Geliþtirme Kampý","Engin Demirog","Java + React");
		Course course3 = new Course(3,"Programlamaya Giriþ Temel Kurs","Engin Demirog","C# + PYTHON + JAVA vb.");
		Course[] courses = {course1,course2,course3};
				
		CourseManager courseManager = new CourseManager();
		
		System.out.println("***Katagoriler...*** : ");
		
		for(Category category : categories) {

			courseManager.printCategoryDetails(category);
			System.out.println("-----------------");
		}
		
		System.out.println("***Kurslar... :*** ");
		for(Course course : courses) {
			courseManager.printCourseDetails(course);
			System.out.println("-----------------");
		}
		courseManager.courseStart(course1);
		courseManager.courseExit(course1);
	}
	

}
