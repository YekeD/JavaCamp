
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sayi = 25;
		for( int i = 2; i<sayi;) {
			int n = sayi % i;
			if (n == 0 ) {
				System.out.println("Sayı asal değildir");
				
			}else {
				System.out.println("Sayı asaldır");
				
			}
		}
	}

}
