package oopIntro;

public class Product {
	int id; //this.id
	String name; //this.name
	double unitPrice; //this.unitPrice
	String detail; //this.detail
	double discount; //this.discount
	double unitPriceAfterDiscount; 
	//Constructor 
	
	public Product(int id, String name, double unitPrice, String detail,double discount,double unitPriceAfterDiscount) {
		super();
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
		this.detail = detail;
		this.discount = discount;
		this.unitPriceAfterDiscount = unitPriceAfterDiscount;
	}

	public Product() {
		System.out.println("Ben Çalýþtým");
	}
	

	
}
